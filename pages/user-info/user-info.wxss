.usi-header{
    width: 100%;
    height: 480rpx;
    position: relative;
}
.totop{
    position: fixed;
    z-index: 1;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 86rpx;
    color: #fff;
    background: #e4007f;
    text-align: center;
    line-height: 86rpx;
}
.usi-header-bgimg{
    width: 100%;
    height: 100%;
}
.usi-header-main{
    position: absolute;
    width: 100%;
    left: 0;
    top: 58rpx;
}
.usi-header-imgwrap{
    width: 130rpx;
    height: 130rpx;
    border: 4rpx solid #fff;
    border-radius: 50%;
    margin: 0 auto;
    overflow: hidden;
}
.usi-header-name{
    font-size: 42rpx;
    line-height: 86rpx;
    text-align: center;
    color: #fff;
}
.usi-header-address{
    text-align: center;
    font-size: 0;
    padding: 8rpx 0;
}
.icon-usi-address{
    width: 17rpx;
    height: 22rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: contain;
    margin-right: 18rpx;
    display: inline-block;
}
.usi-header-address-txt{
    display: inline-block;
    font-size: 28rpx;
    color: #fff;
}
.usi-header-school{
    text-align: center;
    font-size: 0;
    padding: 8rpx 0;
}
.icon-usi-school{
    width: 21rpx;
    height: 21rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: contain;
    margin-right: 18rpx;
    display: inline-block;
}
.usi-header-school-txt{
    display: inline-block;
    font-size: 24rpx;
    color: #fff;
}
.usi-header-bottom{
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    padding: 18rpx 0;
    height: 66rpx;
    line-height: 66rpx;
    background: rgba(0, 0, 0, .5);
}
.usi-hb-ls{
    width: 50%;
    font-size: 24rpx;
    color: #fff;
}
.usi-hb-ls:first-child{
    text-align: center;
}
.usi-hb-right{
    box-sizing: border-box;
    padding: 0 40rpx;
    border-left: 2rpx solid #fff;
}
.usi-hbr-more{
    text-decoration: underline;
}
.usi-cm-title{
    line-height: 60rpx;
    font-size: 26rpx;
    padding: 30rpx;
}
.usi-cm-title-left{
    color: #1c1c1c;
}
.usi-cm-title-right{
    margin-right: 13rpx;
}
.icon-uis-arrow-gray{
    width: 16rpx;
    height: 27rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: contain;
    margin-left: 20rpx;
    margin-top: 19rpx;
}
.usi-cmtr-txt{
    color: #1c1c1c;
}
.usi-photo-title{
    padding-top: 20rpx;
    padding-bottom: 20rpx;
    border-bottom: 2rpx solid #aaaaaa;
}
.uis-photo-main{
    padding: 30rpx;
}
.uis-photo-main-wrap{
    width: 100%;
    overflow: hidden;
}
.uis-photo-body{
    margin-right: -30rpx;
    margin-bottom: -30rpx;
}
.uis-photo-ls{
    width: 33.3%;
    padding-right: 30rpx;
    box-sizing: border-box;
    margin-bottom: 30rpx;
}
.uis-photo-ls-imgwrap{
    height: 175rpx;
}
.uis-photo-ls-last{
    border: 6rpx solid #fbfbfb;
    background: #f6f6f6;
    height: 163rpx;
}
.icon-usi-plus{
    width: 43rpx;
    height: 43rpx;
    background: url(data:image/png;base64,) no-repeat; 
    background-size: contain;
    margin: 60rpx auto 0;
}
.usi-expe-title{
    padding-top: 20rpx;
    padding-bottom: 20rpx;
    border-bottom: 2rpx solid #aaaaaa;
    border-top: 2rpx solid #aaaaaa;
}
.uis-expe-ls{
    height: 215rpx;
}
.uis-expe-ls-imgwrap{
    height: 175rpx;
    position: relative;
    box-shadow: 0 10rpx 10rpx #bbb;
}
.uis-expe-ls-imgwrap::before{
    content: '';
    display: block;
    position: absolute;
    left: 0;
    top: 0;
    height: 100%;
    width: 6rpx;
    background: rgba(0, 0, 0, .6);
}
.uis-expe-ls-num{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    line-height: 175rpx;
    text-align: center;
    color: #fff;
    font-size: 40rpx;
}
.uis-expe-ls{
    text-align: center;
    font-size: 24rpx;
    line-height: 40rpx;
    padding: 0 10px;
    width: 100%;
    box-sizing: border-box;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.uis-expe-ls-last{
    background: #333333;
}
.usi-info-title{
    color: #fff;
    background: #000;
    padding: 0 30rpx;
    line-height: 60rpx;
    font-size: 24rpx;
}
.usi-info-ls{
    font-size: 28rpx;
    border-bottom: 2rpx solid #aaaaaa;
}
.usi-info-habit{
    margin-top: 6rpx;
    line-height: 48rpx;
    padding: 0 20rpx;
    border-radius: 12rpx;
    line-height: 48rpx;
    margin-left: 24rpx;
    color: #fff;
}
.usi-info-tag{
    margin-top: 6rpx;
    line-height: 48rpx;
    padding: 0 26rpx;
    border-radius: 0 8rpx 8rpx 0;
    line-height: 48rpx;
    margin-left: 40rpx;
    color: #fff;
    position: relative;
}
.usi-info-tags .bg-blue::before{
    content: '';
    display: block;
    position: absolute;
    width: 0;
    top: 0;
    left: -44rpx;
    border: 24rpx solid transparent;
    border-right-color: #1c57ff;
}
.usi-info-tags .bg-pink::before{
    content: '';
    display: block;
    position: absolute;
    width: 0;
    top: 0;
    left: -44rpx;
    border: 24rpx solid transparent;
    border-right-color: #e4007f;
}
.bg-blue{
    background: #1c57ff;
}
.bg-pink{
    background: #e4007f;
}
.icon-uis-arrow{
    width: 16rpx;
    height: 27rpx;
    background: url(data:image/png;base64,) no-repeat; 
    background-size: contain;
    margin-left: 20rpx;
    margin-top: 19rpx;
}
.usi-info-ls-sc{
    font-size: 26rpx;
    padding: 0 20rpx;
    line-height: 28rpx;
    margin-top: 16rpx;
    border-left: 2rpx solid #f4f4f4;
}
.usi-info-ls-sc:first-child{
    border-left: none;
}
.usi-info-info{
    padding: 30rpx;
}
.usi-info-info-title{
    font-size: 28rpx;
    color: #1c1c1c;
    line-height: 60rpx;
}
.usi-info-info-content{
    font-size: 26rpx;
    line-height: 34rpx;
    color: #262626;
}
.usi-data-main{
    padding: 30rpx;
}
.usi-data-ls{
    text-align: center;
    font-size: 24rpx;
    line-height: 36rpx;
    padding: 10rpx 0;
}
.usi-data-ls-head{
    color: #ccc;
    font-size: 28rpx;
}
.usi-data-item1{
    width: 20%;
}
.usi-data-item1{
    width: 20%;
}
.usi-data-item2{
    width: 20%;
}
.usi-data-item3{
    width: 15%;
}
.usi-data-item4{
    width: 25%;
}
.usi-data-item5{
    width: 20%;
}
.usi-act-main{
    padding: 13rpx 30rpx 140rpx;
    background: #f3f3f3;
}
.usi-act-ls{
    margin-bottom: 31rpx;
}
.usi-act-ls-title{
    line-height: 40rpx;
    padding: 20rpx 0;
}
.usi-act-ls-mtitle{
    font-size: 30rpx;
    color: #333333;
    margin-right: 30rpx;
}
.usi-act-ls-stitle{
    font-size: 24rpx;
    color: rgba(0, 0, 0, .6)
}
.usi-al-bottom{
    background: #fff;
    line-height: 60rpx;
    padding: 0 18rpx;
    font-size: 28rpx;
    color: #9a9a9a;
}
.usi-al-money{
    width: auto;
    min-width: initial;
    margin-right: 12rpx;
}
.usi-al-bottom-two{
    width: 42%;
}

