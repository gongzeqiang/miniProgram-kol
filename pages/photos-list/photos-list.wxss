page{
    background: #ebebeb;
}
.phl{
    padding: 30rpx;
}
.phl-main{
    overflow-x: hidden;
}
.phl-body{
    margin-right: -30rpx;
}
.phl-ls{
    width: 50%;
    padding-right: 30rpx;
    box-sizing: border-box;
    margin-bottom: 30rpx;
}
.phl-list{
    border: 20rpx solid #fff;
}
.phl-item{
    height: 200rpx;
}
.phl-item > image{
    width: 100%;
    height: 200rpx;
}
